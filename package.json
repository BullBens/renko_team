{
  "name": "renkoTeam",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android_release_apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "android_release_bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "pod_clean": "cd ios && pod deintegrate && pod install && cd ../",
    "postinstall": "npm run pod_clean",
    "watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "android": "react-native run-android",
    "clean": "rimraf $TMPDIR/react-* && yarn cache clean --force && yarn android:clean && yarn ios:clean && rimraf $TMPDIR/react-native-packager-cache-* && rimraf $TMPDIR/metro-bundler-cache-* && rimraf $TMPDIR/metro* && watchman watch-del-all",
    "release": "npx react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "ios:clean": "cd ios && pod deintegrate && pod install && cd ../ ",
    "ios:8": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios:5s": "react-native run-ios --simulator=\"iPhone 5s\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "g:screen": "hygen generate screen",
    "g:modal": "hygen generate modal",
    "g:component": "hygen generate component",
    "g:screen-component": "hygen generate screen-component",
    "g:reducer": "hygen generate reducer"
  },
